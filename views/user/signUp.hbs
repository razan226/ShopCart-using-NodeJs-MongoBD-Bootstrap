<div class="col-md-4 offset-md-4" style="padding-top:60px">
    {{#each massage}}
    <div class="alert alert-danger">
        <p>{{this}}</p></div>
        
    {{/each}}
    <h4 class="text-center" style="padding-bottom: 30px;">Sign-Up </h4>
<form action="signUp" method="post">
    <div class="form-group">
        <label for="email">Email</label>
        <input type="email" name="email" placeholder="Enter Your Email" class="form-control">
    </div>
    <div class="form-group">
        <label for="password">Password</label>
        <input type="password" name="password" placeholder="Enter Your Password" class="form-control">
    </div>
    <div class="form-group">
        <label for="Confirm-password">Confirm-password</label>
        <input type="password" name="Confirm-password" placeholder="Enter Your Password" class="form-control">
    </div>
    <br>
    <input type="hidden" name="_csrf" value="{{tokens}}">
        <button type="submit" class="btn btn-primary" >Sign-Up</button>
  


</form>

</div>